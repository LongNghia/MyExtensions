{
  "name": "logtabs",
  "version": "1.0",
  "description": "a test ext to log all tab in current window",
  "incognito": "split",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },

  "browser_action": {
    "default_icon": "icons/48.png",
    "default_title": "log all tabs!"
  },
  "commands": {
    "logTabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "description": "Toggle feature log tabs"
    },
    "dublicateTab": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      },
      "description": "Duplicate a tab"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "management",
    "contextMenus",
    "bookmarks",
    "file://*/*"
  ],
  "manifest_version": 2
}
